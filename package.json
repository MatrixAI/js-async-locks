{
  "name": "@matrixai/async-locks",
  "version": "1.0.0",
  "author": "Roger Qiu",
  "description": "Asynchronous locking utilities",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-async-locks.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -r ./dist || true; tsc -p ./tsconfig.build.json",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "test": "jest",
    "lint": "eslint '{src,tests}/**/*.{js,ts}'",
    "lintfix": "eslint '{src,tests}/**/*.{js,ts}' --fix",
    "docs": "rm -r ./docs || true; typedoc --gitRevision master --tsconfig ./tsconfig.build.json --out ./docs src && touch ./docs/.nojekyll"
  },
  "dependencies": {
    "@matrixai/resources": "^1.0.0",
    "async-mutex": "^0.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.2.5",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.4.3",
    "typescript-cached-transpile": "0.0.6"
  }
}
