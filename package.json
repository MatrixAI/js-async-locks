{
  "name": "@matrixai/async-locks",
  "version": "2.3.0",
  "author": "Roger Qiu",
  "description": "Asynchronous locking utilities",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-async-locks.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc -p ./tsconfig.build.json",
    "build": "rimraf ./dist && tsc -p ./tsconfig.build.json",
    "postversion": "npm install --package-lock-only --ignore-scripts --silent",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "test": "jest",
    "lint": "eslint '{src,tests}/**/*.{js,ts}'",
    "lintfix": "eslint '{src,tests}/**/*.{js,ts}' --fix",
    "docs": "rimraf ./docs && typedoc --gitRevision master --tsconfig ./tsconfig.build.json --out ./docs src"
  },
  "dependencies": {
    "@matrixai/errors": "^1.1.1",
    "@matrixai/resources": "^1.1.3",
    "async-mutex": "^0.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jest-junit": "^13.2.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.5.2"
  }
}
